{
  "name" : "amaris-etl",
  "jobMode" : "SCRIPT",
  "description" : "ETL process to transform three csv files into one parquet file.",
  "role" : "arn:aws:iam::030334431257:role/amaris-glue-s3",
  "executionProperty" : {
    "maxConcurrentRuns" : 1
  },
  "command" : {
    "name" : "glueetl",
    "scriptLocation" : "s3://aws-glue-assets-030334431257-us-east-1/scripts/amaris-etl.py",
    "pythonVersion" : "3"
  },
  "defaultArguments" : {
    "--enable-metrics" : "true",
    "--enable-spark-ui" : "true",
    "--spark-event-logs-path" : "s3://aws-glue-assets-030334431257-us-east-1/sparkHistoryLogs/",
    "--enable-job-insights" : "true",
    "--enable-observability-metrics" : "true",
    "--enable-glue-datacatalog" : "true",
    "--job-bookmark-option" : "job-bookmark-disable",
    "--job-language" : "python",
    "--TempDir" : "s3://aws-glue-assets-030334431257-us-east-1/temporary/"
  },
  "maxRetries" : 0,
  "allocatedCapacity" : 2,
  "timeout" : 480,
  "maxCapacity" : 2.0,
  "glueVersion" : "5.0",
  "numberOfWorkers" : 2,
  "workerType" : "G.1X",
  "executionClass" : "STANDARD",
  "sourceControlDetails" : {
    "provider" : "GITHUB",
    "repository" : "energy-etl",
    "branch" : "main",
    "folder" : "src"
  }
}